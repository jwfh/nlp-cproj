\RequirePackage{mun-colour}

\mode<presentation>

\setbeamercolor*{palette primary}{use=structure,fg=white,bg=MUNClaret}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=coolGrey10}
\setbeamercolor*{palette tertiary}{use=structure,fg=black,bg=white}
\setbeamercolor*{palette quaternary}{use=structure,fg=coolGrey10,bg=coolGrey7}

\setbeamercolor{navigation symbols}{fg=coolGrey7!75!white}

\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{section in toc}{parent=palette tertiary}
\setbeamercolor*{subsection in toc}{parent=palette tertiary}

\setbeamercolor*{titlelike}{parent=palette secondary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
